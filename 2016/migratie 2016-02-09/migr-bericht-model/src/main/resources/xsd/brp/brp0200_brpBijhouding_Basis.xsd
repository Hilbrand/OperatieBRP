<!-- Wijzigingen PoC Bijhouding 'Fiattering/DualeBijhouding' en Aansluiting IND (verblijfsrecht): -->
<!-- 1) Berichtparameter 'bezienVanuitPersoon' toegevoegd tbv asymmetrische bijhouding door woongemeente -->
<!-- 2) Bijhoudingsplan toegevoegd aan BijhoudingResultaat -->
<!-- 3) Mee kunnen geven van Anr en BSN voor gerelateerde partner -->
<!-- 4) Handeling en Bijhoudingresultaat 'Wijziging verblijfsrecht' toegevoegd -->
<!-- 5) Aanscherping minOccurs 'bron';  indien de container 'bronnen' voor komt in het bericht, dan is hierbij minimaal 1 bron opgenomen -->
<!-- ... -->
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:brp="http://www.bzk.nl/brp/brp0200" targetNamespace="http://www.bzk.nl/brp/brp0200" elementFormDefault="qualified" attributeFormDefault="unqualified" version="020004">
	<include schemaLocation="brp0200_brpBasis.xsd"/>
	<!-- ... -->
	<!-- Alle Berichtelementen Bijhouding die voor meerdere modules een rol spelen. Specifieke vulling van objecten/groepen opgenomen in BRPxxxx_<BZM>.xsd's. -->
	<!-- ... -->
	<!-- Referenties naar Personen in berichten; indien alleen verwijzing naar Ingeschrevene (hoofdpersson) van toepassing is, dan volstaat opgave 'objectsleutel' -->
	<complexType name="Objecttype_Persoon_IdentificatieIngeschrevene_Bijhouding">
		<complexContent>
			<restriction base="brp:Objecttype_Persoon">
				<sequence/>
				<attribute ref="brp:objecttype" use="required" fixed="Persoon"/>
				<attribute ref="brp:verwerkingssoort" use="prohibited"/>
				<attribute ref="brp:objectSleutel" use="required"/>
				<attribute ref="brp:voorkomenSleutel" use="prohibited" />
				<attribute ref="brp:referentieID" use="prohibited"/>
				<attribute ref="brp:communicatieID" use="required"/>
			</restriction>
		</complexContent>
	</complexType>
	<!-- Referenties naar Personen in berichten; in geval van gerelateerden (ouder/kind/partner) is verwijizng naar een 'Ingeschrevene' mogelijk (via objectsleutel); anders -->
	<!-- in geval van een gerelateerde met soortCode 'NietIngesrhrevene'/'Onbekend' de vulling van de identificerende groepen --> 
	<complexType name="Objecttype_Persoon_Identificatie_Bijhouding">
		<complexContent>
			<restriction base="brp:Objecttype_Persoon">
				<sequence>
					<element name="soortCode" type="brp:SoortPersoonCode" nillable="false" minOccurs="0" maxOccurs="1"/> 
					<element name="identificatienummers" type="brp:Groep_PersoonIdentificatienummers_NietIngeschreveneOnbekend" nillable="false" minOccurs="0" maxOccurs="1"/>
					<element name="samengesteldeNaam" type="brp:Groep_PersoonSamengesteldeNaam_NietIngeschreveneOnbekend" nillable="false" minOccurs="0" maxOccurs="1"/>
					<element name="geboorte" type="brp:Groep_PersoonGeboorte_NietIngeschreveneOnbekend" nillable="false" minOccurs="0" maxOccurs="1"/>
					<element name="geslachtsaanduiding" type="brp:Groep_PersoonGeslachtsaanduiding_NietIngeschreveneOnbekend" nillable="false" minOccurs="0" maxOccurs="1"/>
				</sequence>
				<attribute ref="brp:objecttype" use="required" fixed="Persoon"/>
				<attribute ref="brp:verwerkingssoort" use="prohibited"/>
				<attribute ref="brp:objectSleutel" use="optional"/>
				<attribute ref="brp:voorkomenSleutel" use="prohibited" />
				<attribute ref="brp:referentieID" use="prohibited"/>
				<attribute ref="brp:communicatieID" use="required"/>
			</restriction>
		</complexContent>
	</complexType>
