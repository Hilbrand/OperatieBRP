<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:brp="http://www.bzk.nl/brp/brp0200" targetNamespace="http://www.bzk.nl/brp/brp0200" elementFormDefault="qualified" attributeFormDefault="unqualified" version="020001">
	<include schemaLocation="brp0200_bhgNationaliteit_Basis.xsd"/>
	<element name="bhg_natRegistreerNationaliteit" type="brp:Nationaliteit_RegistreerNationaliteit_Bijhouding"/>
	<element name="bhg_natRegistreerNationaliteit_R" type="brp:Nationaliteit_RegistreerNationaliteit_BijhoudingResultaat"/>
	<!-- -->
	<!-- Bericht RegistreerNationaliteit -->
	<complexType name="Nationaliteit_RegistreerNationaliteit_Bijhouding">
		<complexContent>
			<restriction base="brp:Objecttype_Bericht_Bijhouding">
				<sequence>
					<element name="stuurgegevens" type="brp:Stuurgegevens_SynchroonVerzoek" nillable="false" minOccurs="1" maxOccurs="1"/>
					<element name="parameters" type="brp:Groep_BerichtParameters_Bijhouding" nillable="false" minOccurs="1" maxOccurs="1"/>
					<choice>
						<element name="verkrijgingNederlandseNationaliteit" type="brp:Handeling_VerkrijgingNederlandseNationaliteit_Bijhouding" nillable="false" minOccurs="1" maxOccurs="1"/>
						<element name="verkrijgingVreemdeNationaliteit" type="brp:Handeling_VerkrijgingVreemdeNationaliteit_Bijhouding" nillable="false" minOccurs="1" maxOccurs="1"/>
						<element name="wijzigingIndicatieNationaliteit" type="brp:Handeling_WijzigingIndicatieNationaliteit_Bijhouding" nillable="false" minOccurs="1" maxOccurs="1"/>
					</choice>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<!-- -->
	<!-- Administratieve handeling Verkrijging Nederlandse Nationaliteit -->
	<complexType name="Handeling_VerkrijgingNederlandseNationaliteit_Bijhouding">
		<complexContent>
			<restriction base="brp:Handeling_VerkrijgingNederlandseNationaliteit">
				<sequence>
					<element name="partijCode" type="brp:PartijCode" nillable="false" minOccurs="1" maxOccurs="1"/>
					<element name="toelichtingOntlening" type="brp:Ontleningstoelichting" nillable="false" minOccurs="0" maxOccurs="1"/>
					<element name="gedeblokkeerdeMeldingen" type="brp:Container_AdministratieveHandelingGedeblokkeerdeMeldingen_Bijhouding" nillable="false" minOccurs="0" maxOccurs="1"/>
					<element name="bronnen" type="brp:Container_AdministratieveHandelingBronnen_Bijhouding" nillable="false" minOccurs="0" maxOccurs="1"/>
					<element name="acties" type="brp:Container_Handeling_Acties_VerkrijgingNederlandseNationaliteit_Bijhouding" nillable="false" minOccurs="1" maxOccurs="1"/>
				</sequence>
				<attribute ref="brp:objecttype" use="required" fixed="AdministratieveHandeling"/>
				<attribute ref="brp:verwerkingssoort" use="prohibited"/>
				<attribute ref="brp:objectSleutel" use="prohibited"/>
				<attribute ref="brp:voorkomenSleutel" use="prohibited"/>
				<attribute ref="brp:referentieID" use="prohibited"/>
				<attribute ref="brp:communicatieID" use="required"/>
			</restriction>
		</complexContent>
	</complexType>
	<!-- -->
	<!-- Administratieve handeling Verkrijging Vreemde Nationaliteit -->
	<complexType name="Handeling_VerkrijgingVreemdeNationaliteit_Bijhouding">
