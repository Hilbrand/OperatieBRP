<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

    <!-- De enums worden uit het model gehaald en hoeven dus niet bij de reverse engineering worden betrokken.
        Deze worden dus uitgezonderd voor de reverse engineering. -->
    <table-filter match-schema="kern" match-name=".*" exclude="true" />
    <table-filter match-schema="ber" match-name=".*" exclude="true" />
    <table-filter match-schema="brm" match-name=".*" exclude="true" />
    <table-filter match-schema="conv" match-name=".*" exclude="true" />
    <table-filter match-schema="ist" match-name=".*" exclude="true" />
    <table-filter match-schema="lev" match-name=".*" exclude="true" />
    <table-filter match-schema="helper" match-name=".*" exclude="true" />
    <table-filter match-schema="public" match-name=".*" exclude="true" />
    <table-filter match-schema="verconv" match-name=".*" exclude="true" />

    <!-- Enums (uit model dependency) -->
    <table-filter match-schema="autaut" match-name="aandmedium" exclude="true" />
    <table-filter match-schema="autaut" match-name="catalogusoptie" exclude="true" />
    <table-filter match-schema="autaut" match-name="categoriedienst" exclude="true" />
    <table-filter match-schema="autaut" match-name="effectafnemerindicaties" exclude="true" />
    <table-filter match-schema="autaut" match-name="expressie" exclude="true" />
    <table-filter match-schema="autaut" match-name="functie" exclude="true" />
    <table-filter match-schema="autaut" match-name="kanaal" exclude="true" />
    <table-filter match-schema="autaut" match-name="protocolleringsniveau" exclude="true" />
    <table-filter match-schema="autaut" match-name="srtabonnementexpressie" exclude="true" />
    <table-filter match-schema="autaut" match-name="srtautorisatiebesluit" exclude="true" />
    <table-filter match-schema="autaut" match-name="srtbevoegdheid" exclude="true" />
    <table-filter match-schema="autaut" match-name="toestand" exclude="true" />

</hibernate-reverse-engineering>
