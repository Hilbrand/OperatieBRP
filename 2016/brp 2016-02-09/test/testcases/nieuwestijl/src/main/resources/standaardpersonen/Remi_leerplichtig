remi = uitGebeurtenissen {
    geboorte(partij: 34401, aanvang: 20011111, toelichting: '1e kind', registratieDatum: 20011111) {
        op '2001/01/01' te 'Delft' gemeente 503
        geslacht 'MAN'
        namen {
            voornamen 'Remi'
            geslachtsnaam 'Vitalis'
        }
        identificatienummers bsn: ${bsn}, anummer: ${anr}
    }
    ${gebeurtenissen}
}

slaOp(remi)
