        <brp:zendendeSysteem>${stuurgegevens.zendendeSysteem}</brp:zendendeSysteem>
        <brp:referentienummer>${stuurgegevens.referentienummer}</brp:referentienummer>
        <brp:tijdstipVerzending>${stuurgegevens.tijdstipVerzending}</brp:tijdstipVerzending>
    </brp:stuurgegevens>
    <!--Optional:-->
    <brp:parameters brp:communicatieID="${parameters.communicatieID}">
        <brp:abonnementNaam>${parameters.abonnementNaam}</brp:abonnementNaam>
    </brp:parameters>
    <brp:zoekcriteriaPersoon brp:communicatieID="${zoekcriteriaPersoon.communicatieID}">
        <!--Optional:-->
        <brp:burgerservicenummer>${zoekcriteriaPersoon.burgerservicenummer}</brp:burgerservicenummer>
        <!--Optional:-->
        <brp:administratienummer>${zoekcriteriaPersoon.administratienummer}</brp:administratienummer>
        <!--Optional:-->
        <brp:geslachtsnaamstam>${zoekcriteriaPersoon.geslachtsnaamstam}</brp:geslachtsnaamstam>
        <!--Optional:-->
        <brp:geboortedatum>${zoekcriteriaPersoon.geboortedatum}</brp:geboortedatum>
    </brp:zoekcriteriaPersoon>
</brp:lvg_bvgZoekPersoon>
