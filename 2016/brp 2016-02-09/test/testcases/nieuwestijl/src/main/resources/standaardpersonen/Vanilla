def adam    =   Persoon.uitDatabase(bsn: 306867837)
def eva     =   Persoon.uitDatabase(bsn: 306741817)

vanilla = uitGebeurtenissen {
    geboorte(partij: 34401, aanvang: 19800101, toelichting: '1e kind', registratieDatum: 19800101) {
        op '1980/01/01' te 'Delft' gemeente 503
        geslacht 'MAN'
        namen {
            voornamen 'Vanilla'
            geslachtsnaam 'Ice'
        }
        ouders moeder: eva, vader: adam
        identificatienummers bsn: ${bsn}, anummer: ${anr}
    }
    ${gebeurtenissen}
}

slaOp(vanilla)
