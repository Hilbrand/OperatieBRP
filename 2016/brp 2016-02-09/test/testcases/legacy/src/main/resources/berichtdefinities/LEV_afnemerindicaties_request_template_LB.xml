      <brp:lvg_synRegistreerAfnemerindicatie xmlns:brp="http://www.bzk.nl/brp/brp0200">
         <brp:stuurgegevens brp:communicatieID="comid.stuurgegevens00B">
            <brp:zendendePartij>${zendendePartij_zsL1}</brp:zendendePartij>
            <brp:zendendeSysteem>${zendendeSysteem_zsL1}</brp:zendendeSysteem>
            <brp:referentienummer>${referentienummer_sLB1}</brp:referentienummer>
            <brp:tijdstipVerzending>${moment_volgen()}</brp:tijdstipVerzending>
         </brp:stuurgegevens>
         <brp:plaatsingAfnemerindicatie brp:communicatieID="${comid$plaatsingAfnemerindicatie1}"
                                        brp:objecttype="AdministratieveHandeling">
            <brp:partijCode>${partijCode_pLB1}</brp:partijCode>
            <brp:gedeblokkeerdeMeldingen>
               <brp:gedeblokkeerdeMelding brp:communicatieID="${comid$gedeblokkeerdeMelding1}"
										  brp:referentieID="${_refid$gedeblokkeerdeMelding1_}"
                                          brp:objecttype="GedeblokkeerdeMelding">
               <!--1 or more repetitiobrp:-->
                  <brp:regelCode>${regelCode_ggp1}</brp:regelCode>
                  <!--Optional:-->
                  <brp:elementNaam>${elementNaam_ggp1}</brp:elementNaam>
               </brp:gedeblokkeerdeMelding>
            </brp:gedeblokkeerdeMeldingen>
            <brp:acties>
               <brp:registratieAfnemerindicatie brp:communicatieID="${comid$registratieAfnemerindicatie1}" brp:objecttype="Actie">
                  <brp:persoon brp:communicatieID="${comid$persoon1}" brp:objecttype="Persoon">
                     <brp:identificatienummers brp:communicatieID="${comid$identificatienummers1}">
                        <brp:burgerservicenummer>${burgerservicenummer_ipr1}</brp:burgerservicenummer>
                     </brp:identificatienummers>
                     <brp:afnemerindicaties>
                        <brp:afnemerindicatie brp:communicatieID="${comid$afnemerindicatie1}"
                                              brp:objecttype="PersoonAfnemerindicatie">
                           <brp:afnemerCode>${afnemerCode_aap1}</brp:afnemerCode>
                           <brp:abonnementNaam>${abonnementNaam_aap1}</brp:abonnementNaam>
                          <brp:datumAanvangMaterielePeriode>${datumAanvangMaterielePeriode_aap1}</brp:datumAanvangMaterielePeriode>
                          <brp:datumEindeVolgen>${datumTijdEindeVolgen_aap1}</brp:datumEindeVolgen>
                        </brp:afnemerindicatie>
                     </brp:afnemerindicaties>
                  </brp:persoon>
               </brp:registratieAfnemerindicatie>
            </brp:acties>
         </brp:plaatsingAfnemerindicatie>
         <brp:verwijderingAfnemerindicatie brp:communicatieID="${comid$verwijderingAfnemerindicatie1}"
                                           brp:objecttype="AdministratieveHandeling">
            <brp:partijCode>${partijCode_vLB1}</brp:partijCode>
            <!--Optional:-->
            <brp:gedeblokkeerdeMeldingen>
               <!--1 or more repetitions:-->
               <brp:gedeblokkeerdeMelding brp:communicatieID="${comid$gedeblokkeerdeMelding2}"
										  brp:referentieID="${_refid$gedeblokkeerdeMelding2_}"
                                          brp:objecttype="GedeblokkeerdeMelding">
                  <brp:regelCode>${regelCode_ggv1}</brp:regelCode>
                  <!--Optional:-->
                  <brp:elementNaam>${elementNaam_ggv1}</brp:elementNaam>
               </brp:gedeblokkeerdeMelding>
            </brp:gedeblokkeerdeMeldingen>
            <brp:acties>
               <!--1 or more repetitions:-->
               <brp:vervalAfnemerindicatie brp:communicatieID="${comid$vervalAfnemerindicatie1}" brp:objecttype="Actie">
                  <brp:persoon brp:communicatieID="${comid$persoon2}" brp:objecttype="Persoon">
                     <brp:identificatienummers brp:communicatieID="${comid$identificatienummers2}">
                        <brp:burgerservicenummer>${burgerservicenummer_ipv1}</brp:burgerservicenummer>
                     </brp:identificatienummers>
                     <brp:afnemerindicaties>
               			<!--1 or more repetitions:-->
                        <brp:afnemerindicatie brp:communicatieID="${comid$afnemerindicatie2}"
                                              brp:objecttype="PersoonAfnemerindicatie">
                           <brp:afnemerCode>${afnemerCode_aap2}</brp:afnemerCode>
                           <brp:abonnementNaam>${abonnementNaam_aap2}</brp:abonnementNaam>
                        </brp:afnemerindicatie>
                     </brp:afnemerindicaties>
                  </brp:persoon>
               </brp:vervalAfnemerindicatie>
            </brp:acties>
         </brp:verwijderingAfnemerindicatie>
      </brp:lvg_synRegistreerAfnemerindicatie>