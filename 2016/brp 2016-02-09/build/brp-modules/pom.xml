    <packaging>pom</packaging>

    <name>BRP Modules</name>
    <description>Deze module bevat de submodules waaruit de BRP software is opgemaakt. Deze module wordt gebruikt ten
        behoeve van de code analyse met Sonar.
    </description>

    <organization>
        <name>Ministerie van Binnenlandse Zaken en Koninkrijksrelaties</name>
        <url>http://www.operatiebrp.nl/</url>
    </organization>

    <ciManagement>
        <system>Jenkins</system>
        <url>https://www.modernodam.nl/ci</url>
    </ciManagement>

    <issueManagement>
        <system>Jira</system>
        <url>https://www.modernodam.nl/jira/browse/TEAMBRP</url>
    </issueManagement>

    <scm>
        <connection>scm:svn:https://www.modernodam.nl/svn/brp-code/trunk/brp-modules</connection>
        <developerConnection>scm:svn:https://www.modernodam.nl/svn/brp-code/trunk/brp-modules</developerConnection>
    </scm>

    <properties>
        <scm.methode>export</scm.methode>
        <!-- De gegenereerde code voor de expressietaal door ANTLR wordt genegeerd voor alle code kwaliteits metingen -->
        <sonar.exclusions>**/antlr/**,**/package-info.java</sonar.exclusions>
        <sonar.cpd.exclusions>**/antlr/**</sonar.cpd.exclusions>
    </properties>
    <build>
        <plugins>
            <plugin>
                <artifactId>maven-antrun-plugin</artifactId>
                <executions>
                    <execution>
                        <id>maak-target-dir</id>
                        <phase>initialize</phase>
                        <configuration>
                            <tasks>
                                <mkdir dir="target" />
                            </tasks>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>cobertura-maven-plugin</artifactId>
                <version>2.6</version>
                <configuration>
                    <formats>
                        <format>xml</format>
                    </formats>
                    <aggregate>true</aggregate>
                    <check>
                        <branchRate>90</branchRate>
                        <lineRate>90</lineRate>
                        <haltOnFailure>false</haltOnFailure>
                        <totalBranchRate>90</totalBranchRate>
                        <totalLineRate>90</totalLineRate>
                        <packageLineRate>90</packageLineRate>
                        <packageBranchRate>90</packageBranchRate>
                    </check>

                </configuration>
            </plugin>
        </plugins>
    </build>

