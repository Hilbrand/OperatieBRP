<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
    http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
">

    <preConditions onFail="HALT">
        <dbms type="hsqldb"/>
    </preConditions>

    <changeSet id="1" author="brp">
        <sqlFile path="hsqldb/HSQLDB.sql" stripComments="true"/>
        <sqlFile path="hsqldb/custom_changes.sql" stripComments="true"/>
        <sqlFile path="hsqldb/Creeer_locking_functions.sql" />
    </changeSet>

    <changeSet id="99" author="brp">
        <comment>Housekeeping.</comment>
        <sqlFile path="kern/99-brp-version.sql"/>
    </changeSet>

    <changeSet id="100" author="brp">
        <tagDatabase tag="${versie} (${timestamp})"/>
    </changeSet>
</databaseChangeLog>
