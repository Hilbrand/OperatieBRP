<?xml version="1.0" encoding="UTF-8"?>
<!--
  Deze binding file is bedoeld voor de extra handmatige binding die nodig is naast de gegenereerde binding.
  Het bevat informatie over binding die niet uit het BMR gehaald kan worden.
 -->
<binding>
    <mapping class="nl.bzk.brp.model.basis.AbstractBerichtIdentificeerbaar" abstract="true"
             type-name="GroepIdentificeerbaar" post-set="jibxPostSet">
        <value style="attribute" name="communicatieID" get-method="getCommunicatieID" set-method="setCommunicatieID"
               usage="optional"/>
    </mapping>
    <mapping class="nl.bzk.brp.model.basis.AbstractBerichtEntiteit" abstract="true"
             type-name="ObjectTypeIdentificeerbaar">
        <structure map-as="AbstractBerichtEntiteit"/>
    </mapping>
    <mapping class="nl.bzk.brp.model.basis.AbstractBerichtEntiteit" abstract="true" type-name="AbstractBerichtEntiteit"
             post-set="jibxPostSet">
        <value style="attribute" name="objecttype" field="objecttype" usage="optional"/>
        <value style="attribute" name="objectSleutel" field="objectSleutel" usage="optional"/>
        <value style="attribute" name="referentieID" field="referentieID" usage="optional"/>
        <value style="attribute" name="communicatieID" get-method="getCommunicatieID" set-method="setCommunicatieID"
               usage="optional"/>
    </mapping>
</binding>
