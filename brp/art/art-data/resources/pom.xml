    </parent>

    <artifactId>brp-art-data-resources</artifactId>
    <packaging>jar</packaging>
    <name>BRP ART Data - Resources</name>

    <scm>
        <connection>scm:svn:https://www.modernodam.nl/svn/brp-code/trunk/art/art-data/resources</connection>
    </scm>

    <build>
        <resources>
            <resource>
                <filtering>false</filtering>
                <directory>./resources</directory>
                <includes>
                    <include>**/*</include>
