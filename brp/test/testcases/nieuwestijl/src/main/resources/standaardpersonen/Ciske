def eva = uitGebeurtenissen {
    geboorte(partij: 34401, aanvang: 19650808, toelichting: '1e kind', registratieDatum: 19650808) {
        op '1965/08/08' te 'Delft' gemeente 503
        geslacht 'VROUW'
        namen {
            voornamen 'Moeder-UC_Kenny'
            geslachtsnaam 'McCormick'
        }
        ouders()
        identificatienummers bsn: 743274313, anummer: 1692461906
    }
}
slaOp(eva)

ciske = uitGebeurtenissen {
    geboorte(partij: 34401, aanvang: 19930916, toelichting: '1e kind', registratieDatum: 19930916) {
        op '1980/01/01' te 'Delft' gemeente 503
        geslacht 'MAN'
        namen {
            voornamen 'Ciske'
            geslachtsnaam 'Rat'
        }
        ouders moeder: eva
        identificatienummers bsn: ${bsn}, anummer: ${anr}
    }
    ${gebeurtenissen}
}

slaOp(ciske)
