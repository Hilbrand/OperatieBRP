    <table-filter match-schema="kern" match-name="predicaat" exclude="true" />
    <table-filter match-schema="kern" match-name="rdneinderelatie" exclude="true" />
    <table-filter match-schema="kern" match-name="rdnopschorting" exclude="true" />
    <table-filter match-schema="kern" match-name="rdnwijzverblijf" exclude="true" />
    <table-filter match-schema="kern" match-name="rol" exclude="true" />
    <table-filter match-schema="kern" match-name="srtactie" exclude="true" />
    <table-filter match-schema="kern" match-name="srtadmhnd" exclude="true" />
    <table-filter match-schema="kern" match-name="srtbetr" exclude="true" />
