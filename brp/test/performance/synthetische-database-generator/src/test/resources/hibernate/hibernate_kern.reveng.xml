    <table-filter match-schema="kern" match-name="srtnlreisdoc" exclude="true" />
    <table-filter match-schema="kern" match-name="srtpartij" exclude="true" />
    <table-filter match-schema="kern" match-name="srtpartijonderzoek" exclude="true" />
    <table-filter match-schema="kern" match-name="srtpers" exclude="true" />
    <table-filter match-schema="kern" match-name="srtpersonderzoek" exclude="true" />
