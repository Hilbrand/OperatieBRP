            <element name="tijdstipOntlening" type="brp:DatumTijd-s"  minOccurs="1" maxOccurs="1" />
            <element name="toelichtingOntlening" type="brp:Ontleningstoelichting-s"  minOccurs="0" maxOccurs="1" />
            <element name="bronnen" type="brp:Container_ActieBronnen_Bijhouding"  minOccurs="0" maxOccurs="1" />
            <element name="persoon" type="brp:Objecttype_Persoon"  minOccurs="1" maxOccurs="1" />
        </sequence>
