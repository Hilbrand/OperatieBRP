<?xml version="1.0" encoding="UTF-8"?>
<!-- Datum:20-09-2012 Status:Concept -->
<schema xmlns="http://www.w3.org/2001/XMLSchema"
xmlns:xs="http://www.w3.org/2001/XMLSchema" 
        xmlns:brp="http://www.bprbzk.nl/BRP/0001"
        xmlns:mig="http://www.moderniseringgba.nl/Migratie/0001" 
        targetNamespace="http://www.moderniseringgba.nl/Migratie/0001" 
        elementFormDefault="qualified" 
        attributeFormDefault="unqualified" 
        version="000101">

    <import schemaLocation="brp/BRP_Afstamming_Basis.xsd" namespace="http://www.bprbzk.nl/BRP/0001" />

    <!-- Toegevoegd aan alle (inhoudelijke) berichten om aan te geven -->
    <!-- tussen welke gemeenten wordt gecommuniceerd.                 -->
    <complexType name="IscGemeenten">
        <sequence>
            <element name="lo3Gemeente" type="brp:Gemeentecode-s" />
            <element name="brpGemeente" type="brp:Gemeentecode-s" />
        </sequence>
    </complexType>
	
	<!-- Toegevoegd ipv IscGemeenten als BRP niet het doel kan bepalen en dat door ISC wordt gedaan -->
	<complexType name="IscGemeentenEnkelBron">
		<sequence>
			<element name="brpGemeente" type="brp:Gemeentecode-s" />
		</sequence>
	</complexType>    

    <complexType name="BrpAntwoordType">
        <sequence>
            <element name="status" type="mig:StatusType"  minOccurs="1" maxOccurs="1"/>
            <element name="toelichting" type="brp:Meldingtekst-s"  minOccurs="0" maxOccurs="1" />
        </sequence>
    </complexType>
    
    
    <!-- Geboorte (BRP -> ISC) -->

    <element name="geboorteVerzoek" type="mig:GeboorteVerzoekType" />
    <!-- Net als brp:View_InschrijvingGeboorte_Afstamming, met toevoeging -->
    <!-- van de bron- en bestemmingsgemeenten (en later misschien nog meer). -->
    <complexType name="GeboorteVerzoekType">
        <sequence>
            <element name="iscGemeenten" type="mig:IscGemeentenEnkelBron"  minOccurs="1" maxOccurs="1" />
            <element name="datumAanvangGeldigheid" type="brp:Datum-s"  minOccurs="1" maxOccurs="1" />
            <element name="tijdstipOntlening" type="brp:DatumTijd-s"  minOccurs="1" maxOccurs="1" />
            <element name="toelichtingOntlening" type="brp:Ontleningstoelichting-s"  minOccurs="0" maxOccurs="1" />
            <element name="bronnen" type="brp:Container_ActieBronnen_Bijhouding"  minOccurs="0" maxOccurs="1" />
            <element name="familierechtelijkeBetrekking" type="brp:View_FamilierechtelijkeBetrekking_Afstamming"  minOccurs="1" maxOccurs="1" />
        </sequence>
    </complexType>

    <element name="geboorteAntwoord" type="mig:GeboorteAntwoordType"/>
    <complexType name="GeboorteAntwoordType">
        <complexContent>
            <extension base="mig:BrpAntwoordType"/>
        </complexContent>
    </complexType>

    <!-- MV Geboorte (ISC- > BRP)-->

    <element name="mvGeboorteVerzoek" type="mig:MvGeboorteVerzoekType" />
    <!-- Net als brp:View_InschrijvingGeboorte_Afstamming, met toevoeging -->
    <!-- van de bron- en bestemmingsgemeenten (en later misschien nog meer). -->
    <complexType name="MvGeboorteVerzoekType">
        <sequence>
            <element name="iscGemeenten" type="mig:IscGemeenten"  minOccurs="1" maxOccurs="1" />
            <element name="datumAanvangGeldigheid" type="brp:Datum-s"  minOccurs="0" maxOccurs="1" />
            <element name="tijdstipOntlening" type="brp:DatumTijd-s"  minOccurs="0" maxOccurs="1" />
            <element name="toelichtingOntlening" type="brp:Ontleningstoelichting-s"  minOccurs="0" maxOccurs="1" />
            <element name="bronnen" type="brp:Container_ActieBronnen_Bijhouding"  minOccurs="0" maxOccurs="1" />
            <element name="familierechtelijkeBetrekking" type="brp:View_FamilierechtelijkeBetrekking_Afstamming"  minOccurs="1" maxOccurs="1" />
        </sequence>
    </complexType>

    <element name="mvGeboorteAntwoord" type="mig:MvGeboorteAntwoordType"/>
    <complexType name="MvGeboorteAntwoordType">
        <sequence>
            <element name="status" type="mig:StatusType"  minOccurs="1" maxOccurs="1"/>
            <element name="toelichting" type="brp:Meldingtekst-s"  minOccurs="0" maxOccurs="1" />
            <element name="foutCode" type="mig:FoutCode"  minOccurs="0" maxOccurs="1" />
            <element name="aNummer" type="brp:ANummer-s"  minOccurs="1" maxOccurs="1" />
        </sequence>
    </complexType>
    
	<!-- Verhuizing (BRP -> ISC) -->
	
	<element name="verhuizingVerzoek" type="mig:VerhuizingVerzoekType"/>
	<complexType name="VerhuizingVerzoekType">
		<sequence>
            <element name="iscGemeenten" type="mig:IscGemeenten"  minOccurs="1" maxOccurs="1" />
			<element name="afzender" type="mig:AfzenderType"  minOccurs="1" maxOccurs="1" />
		    <element name="aNummer" type="brp:ANummer-s"  minOccurs="1" maxOccurs="1"/>
			<element name="burgerservicenummer" type="brp:Burgerservicenummer-s"  minOccurs="1" maxOccurs="1"/>
			<element name="huidigeGemeente" type="brp:Gemeentecode-s"  minOccurs="1" maxOccurs="1" />
			<element name="nieuweGemeente" type="brp:Gemeentecode-s"  minOccurs="1" maxOccurs="1" />
			<element name="datumInschrijving" type="brp:Datum-s"  minOccurs="1" maxOccurs="1" />
			<element name="adresseerbaarObject" type="mig:AdresseerbaarObjectType"  minOccurs="0" maxOccurs="1" />
			<element name="idNummeraanduiding" type="brp:IdentificatiecodeNummeraanduiding-s"  minOccurs="0" maxOccurs="1" />
			<element name="naamOpenbareRuimte" type="brp:NaamOpenbareRuimte-s"  minOccurs="0" maxOccurs="1" />
			<element name="afgekorteNaamOpenbareRuimte" type="brp:AfgekorteNaamOpenbareRuimte-s"  minOccurs="0" maxOccurs="1" />
			<element name="huisnummer" type="brp:Huisnummer-s"  minOccurs="0" maxOccurs="1" />
			<element name="huisletter" type="brp:Huisletter-s"  minOccurs="0" maxOccurs="1" />
			<element name="huisnummertoevoeging" type="brp:Huisnummertoevoeging-s"  minOccurs="0" maxOccurs="1" />
			<element name="postcode" type="brp:Postcode-s"  minOccurs="0" maxOccurs="1" />
			<element name="woonplaats" type="mig:WoonplaatsType"  minOccurs="0" maxOccurs="1" />
			<element name="locatieTovAdres" type="mig:LocatieTovAdresType"  minOccurs="0" maxOccurs="1" />
			<element name="locatieOmschrijving" type="brp:LocatieOmschrijving-s"  minOccurs="0" maxOccurs="1" />
		</sequence>
	</complexType>	
	
	<element name="verhuizingAntwoord" type="mig:VerhuizingAntwoordType"/>
	<complexType name="VerhuizingAntwoordType">
        <complexContent>
            <extension base="mig:BrpAntwoordType"/>
        </complexContent>
	</complexType>

	<!-- MV Verhuizing (ISC -> BRP) -->
	
	<element name="mvVerhuizingVerzoek" type="mig:MvVerhuizingVerzoekType"/>
	<complexType name="MvVerhuizingVerzoekType">
		<sequence>
            <element name="iscGemeenten" type="mig:IscGemeenten"  minOccurs="1" maxOccurs="1" />
			<element name="afzender" type="mig:AfzenderType"  minOccurs="1" maxOccurs="1" />
