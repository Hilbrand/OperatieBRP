			<element name="burgerservicenummer" type="brp:Burgerservicenummer-s"  minOccurs="1" maxOccurs="1"/>
			<element name="huidigeGemeente" type="brp:Gemeentecode-s"  minOccurs="1" maxOccurs="1" />
			<element name="nieuweGemeente" type="brp:Gemeentecode-s"  minOccurs="1" maxOccurs="1" />
			<element name="datumInschrijving" type="brp:Datum-s"  minOccurs="1" maxOccurs="1" />
			<element name="adresseerbaarObject" type="mig:AdresseerbaarObjectType"  minOccurs="0" maxOccurs="1" />
			<element name="idNummeraanduiding" type="brp:IdentificatiecodeNummeraanduiding-s"  minOccurs="0" maxOccurs="1" />
			<element name="naamOpenbareRuimte" type="brp:NaamOpenbareRuimte-s"  minOccurs="0" maxOccurs="1" />
			<element name="afgekorteNaamOpenbareRuimte" type="brp:AfgekorteNaamOpenbareRuimte-s"  minOccurs="0" maxOccurs="1" />
			<element name="huisnummer" type="brp:Huisnummer-s"  minOccurs="0" maxOccurs="1" />
			<element name="huisletter" type="brp:Huisletter-s"  minOccurs="0" maxOccurs="1" />
			<element name="huisnummertoevoeging" type="brp:Huisnummertoevoeging-s"  minOccurs="0" maxOccurs="1" />
			<element name="postcode" type="brp:Postcode-s"  minOccurs="0" maxOccurs="1" />
			<element name="woonplaats" type="mig:WoonplaatsType"  minOccurs="0" maxOccurs="1" />
			<element name="locatieTovAdres" type="mig:LocatieTovAdresType"  minOccurs="0" maxOccurs="1" />
			<element name="locatieOmschrijving" type="brp:LocatieOmschrijving-s"  minOccurs="0" maxOccurs="1" />
		</sequence>
	</complexType>	
	
	<element name="mvVerhuizingAntwoord" type="mig:MvVerhuizingAntwoordType"/>
	<complexType name="MvVerhuizingAntwoordType">
        <complexContent>
            <extension base="mig:BrpAntwoordType"/>
        </complexContent>
	</complexType>

	<!-- Zoek persoon (ISC -> BRP) -->
	
	<element name="zoekPersoonVerzoek" type="mig:ZoekPersoonVerzoekType"/>
	<complexType name="ZoekPersoonVerzoekType">
		<sequence>
			<element name="aNummer" type="brp:ANummer-s"  minOccurs="0" maxOccurs="1"/>
			<element name="burgerservicenummer" type="brp:Burgerservicenummer-s"  minOccurs="0" maxOccurs="1"/>
			<element name="geslachtsnaam" type="brp:Geslachtsnaam-s"  minOccurs="0" maxOccurs="1"/>
			<element name="geboortedatum" type="brp:Datum-s"  minOccurs="0" maxOccurs="1"/>
			<element name="geslachtsaanduiding" type="brp:GeslachtsaanduidingCode-s"  minOccurs="0" maxOccurs="1"/>
			<element name="voornamen" type="brp:Voornamen-s"  minOccurs="0" maxOccurs="1"/>
			<element name="postcode" type="brp:Postcode-s"  minOccurs="0" maxOccurs="1"/>
			<element name="bijhoudingsgemeente" type="brp:Gemeentecode-s"  minOccurs="0" maxOccurs="1"/>
		</sequence>
	</complexType>
	
	<element name="zoekPersoonAntwoord" type="mig:ZoekPersoonAntwoordType"/>
	<complexType name="ZoekPersoonAntwoordType">
		<sequence>
			<element name="status" type="mig:StatusType" minOccurs="1" maxOccurs="1"/>
			<element name="toelichting" type="brp:Meldingtekst-s"  minOccurs="0" maxOccurs="1" />		
			<element name="gevondenPersonen" type="mig:GevondenPersonenType"  minOccurs="0" maxOccurs="1"/>
		</sequence>
	</complexType>
	
	<complexType name="GevondenPersonenType">
		<sequence>
			<element name="gevondenPersoon" type="mig:GevondenPersoonType"  minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
	<complexType name="GevondenPersoonType">
		<sequence>
			<element name="aNummer" type="brp:ANummer-s"  minOccurs="1" maxOccurs="1"/>
			<element name="bijhoudingsgemeente" type="brp:Gemeentecode-s"  minOccurs="1" maxOccurs="1"/>
		</sequence>
	</complexType>
	
	<!-- Notificatie (ISC -> BRP) -->
	
	<element name="notificatieVerzoek" type="mig:NotificatieVerzoekType"/>
	<complexType name="NotificatieVerzoekType">
		<sequence>
			<element name="notificatie" type="brp:Meldingtekst-s"  minOccurs="1" maxOccurs="1"/>
		</sequence>
	</complexType>
	<element name="notificatieAntwoord" type="mig:NotificatieAntwoordType"/>
	<complexType name="NotificatieAntwoordType">
        <complexContent>
            <extension base="mig:BrpAntwoordType"/>
        </complexContent>
	</complexType>

	<!-- Synchronisatie signaal (BRP -> ISC) -->
	
	<element name="synchronisatieSignaal" type="mig:SynchronisatieSignaalType"/>
	<complexType name="SynchronisatieSignaalType">
		<sequence>
			<element name="aNummer" type="brp:ANummer-s"  minOccurs="1" maxOccurs="1"/>
		</sequence>
	</complexType>
 
    <!-- Erkenning na geboorte (BRP -> ISC) -->

    <element name="erkenningVerzoek" type="mig:ErkenningVerzoekType" />
    <complexType name="ErkenningVerzoekType">
        <sequence>
            <element name="iscGemeenten" type="mig:IscGemeenten"  minOccurs="1" maxOccurs="1" />
            <element name="datumAanvangGeldigheid" type="brp:Datum-s"  minOccurs="1" maxOccurs="1" />
            <element name="tijdstipOntlening" type="brp:DatumTijd-s"  minOccurs="1" maxOccurs="1" />
            <element name="toelichtingOntlening" type="brp:Ontleningstoelichting-s"  minOccurs="0" maxOccurs="1" />
            <element name="bronnen" type="brp:Container_ActieBronnen_Bijhouding"  minOccurs="0" maxOccurs="1" />
            <element name="kind" type="brp:View_Kind_Afstamming"  minOccurs="1" maxOccurs="1" />
            <element name="erkenner" type="brp:View_Ouder_Afstamming"  minOccurs="1" maxOccurs="1" />
        </sequence>
    </complexType>

    <element name="erkenningAntwoord" type="mig:ErkenningAntwoordType"/>
    <complexType name="ErkenningAntwoordType">
        <complexContent>
            <extension base="mig:BrpAntwoordType"/>
        </complexContent>
    </complexType>

    <!-- Erkenning na geboorte via notariële akte (BRP -> ISC) -->

    <element name="erkenningNotarieelVerzoek" type="mig:ErkenningNotarieelVerzoekType" />
    <complexType name="ErkenningNotarieelVerzoekType">
        <sequence>
            <element name="iscGemeenten" type="mig:IscGemeenten"  minOccurs="1" maxOccurs="1" />
            <element name="datumAanvangGeldigheid" type="brp:Datum-s"  minOccurs="1" maxOccurs="1" />
            <element name="tijdstipOntlening" type="brp:DatumTijd-s"  minOccurs="1" maxOccurs="1" />
            <element name="toelichtingOntlening" type="brp:Ontleningstoelichting-s"  minOccurs="0" maxOccurs="1" />
            <element name="bronnen" type="brp:Container_ActieBronnen_Bijhouding"  minOccurs="0" maxOccurs="1" />
            <element name="kind" type="brp:View_Kind_Afstamming"  minOccurs="1" maxOccurs="1" />
            <element name="erkenner" type="brp:View_Ouder_Afstamming"  minOccurs="1" maxOccurs="1" />
        </sequence>
    </complexType>

    <element name="erkenningNotarieelAntwoord" type="mig:ErkenningNotarieelAntwoordType"/>
    <complexType name="ErkenningNotarieelAntwoordType">
        <complexContent>
            <extension base="mig:BrpAntwoordType"/>
        </complexContent>
    </complexType>

    <!-- Vernietiging erkenning (BRP -> ISC) -->

    <element name="erkenningVernietigingVerzoek" type="mig:ErkenningVernietigingVerzoekType" />
    <complexType name="ErkenningVernietigingVerzoekType">
        <sequence>
            <element name="iscGemeenten" type="mig:IscGemeenten"  minOccurs="1" maxOccurs="1" />
            <element name="datumAanvangGeldigheid" type="brp:Datum-s"  minOccurs="1" maxOccurs="1" />
            <element name="tijdstipOntlening" type="brp:DatumTijd-s"  minOccurs="1" maxOccurs="1" />
            <element name="toelichtingOntlening" type="brp:Ontleningstoelichting-s"  minOccurs="0" maxOccurs="1" />
            <element name="bronnen" type="brp:Container_ActieBronnen_Bijhouding"  minOccurs="0" maxOccurs="1" />
            <element name="kind" type="brp:View_Kind_Afstamming"  minOccurs="1" maxOccurs="1" />
        </sequence>
    </complexType>

    <element name="erkenningVernietigingAntwoord" type="mig:ErkenningVernietigingAntwoordType"/>
    <complexType name="ErkenningVernietigingAntwoordType">
        <complexContent>
            <extension base="mig:BrpAntwoordType"/>
        </complexContent>
    </complexType>

    <!-- ontkenning vaderschap (1*E) (BRP -> ISC) -->

    <element name="ontkenningVaderschapVerzoek" type="mig:OntkenningVaderschapVerzoekType" />
    <complexType name="OntkenningVaderschapVerzoekType">
        <sequence>
            <element name="iscGemeenten" type="mig:IscGemeenten"  minOccurs="1" maxOccurs="1" />
            <element name="datumAanvangGeldigheid" type="brp:Datum-s"  minOccurs="1" maxOccurs="1" />
            <element name="tijdstipOntlening" type="brp:DatumTijd-s"  minOccurs="1" maxOccurs="1" />
            <element name="toelichtingOntlening" type="brp:Ontleningstoelichting-s"  minOccurs="0" maxOccurs="1" />
            <element name="bronnen" type="brp:Container_ActieBronnen_Bijhouding"  minOccurs="0" maxOccurs="1" />
            <element name="kind" type="brp:View_Kind_Afstamming"  minOccurs="1" maxOccurs="1" />
            <element name="vader" type="brp:View_Ouder_Afstamming"  minOccurs="1" maxOccurs="1" />
        </sequence>
    </complexType>
    
    <!--  Signal wijziging a-nummer (BRP -> ISC) -->
    
	<element name="wijzigingANummerSignaal" type="mig:WijzigingANummerSignaalType"/>
	<complexType name="WijzigingANummerSignaalType">
		<sequence>
			<element name="iscGemeenten" type="mig:IscGemeentenEnkelBron" minOccurs="1" maxOccurs="1"/>
