            <xml-property name="queryText"><![CDATA[select
  definition.id_ as "Proces_id",
  definition.name_ as "Proces",
  definition.version_ as "Versie",
  coalesce(sum(uitvoeringminder),0) as "In uitvoering 2 dagen of korter",
  coalesce(sum(uitvoeringmeer),0) as "In uitvoering > 2 dagen",
  coalesce(sum(wachtminder),0) as "Wacht op actie beheerder 1 dag of korter",
  coalesce(sum(wachtmeer),0) as "Wacht op actie beheerder > 1 dag"
from
  jbpm_processdefinition definition
  left join jbpm_processinstance instance on definition.id_=instance.processdefinition_
  left join (
  
select
  instance.id_,
  case when now()-max(instance.start_)<=interval '2 day' then 1 else 0 end as uitvoeringminder,
  case when now()-max(instance.start_)>interval '2 day' then 1 else 0 end as uitvoeringmeer,
  case when now()-max(token.start_)<=interval '1 day' then 1 else 0 end as wachtminder, -- er is een token die een dag of korter in een task node zit
  case when now()-min(token.start_)>interval '1 day' then 1 else 0 end as wachtmeer -- er is een token die langer dan een dag in een task node zit
from
  jbpm_processinstance instance
  inner join jbpm_token token on token.processinstance_=instance.id_
  inner join jbpm_node node on token.node_=node.id_ and node.class_='K'
where
  token.end_ is null -- loopt nog
group by instance.id_

union

select
  instance.id_,
  case when now()-max(instance.start_)<=interval '2 day' then 1 else 0 end as uitvoeringminder,
  case when now()-max(instance.start_)>interval '2 day' then 1 else 0 end as uitvoeringmeer,
  case when now()-max(token.start_)<=interval '1 day' then 1 else 0 end as wachtminder, -- er is een token die een dag of korter in een task node zit
  case when now()-min(token.start_)>interval '1 day' then 1 else 0 end as wachtmeer -- er is een token die langer dan een dag in een task node zit
from
  jbpm_processinstance instance
  inner join jbpm_token token on token.processinstance_=instance.id_
  inner join jbpm_node node on token.node_=node.id_ and node.class_='C'
where
  exists (select 1 from jbpm_token subtoken inner join jbpm_node subnode on token.subprocessinstance_=subtoken.processinstance_ and subtoken.node_=subnode.id_ and subnode.class_='K')
  and token.end_ is null -- loopt nog
group by instance.id_

  ) as us47 on instance.id_=us47.id_

where
  definition.id_ in (select processdefinition_ from jbpm_processinstance where superprocesstoken_ is null)
group by definition.id_,definition.name_,definition.version_
order by definition.id_,definition.name_,definition.version_]]></xml-property>
