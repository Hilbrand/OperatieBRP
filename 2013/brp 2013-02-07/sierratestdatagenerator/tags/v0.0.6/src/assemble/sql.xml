<!-- Voegt het gegenereerde sql bestand 'whiteboxfiller.sql' toe aan de jar -->
<assembly>
	<!-- naam wordt ${artifactId}-${version}-${id}.zip -->
	<id>sql</id>
	<formats>
		<format>zip</format>
	</formats>
	<files>
		<file>
			<!-- move bestand naar de root om te zippen -->
			<source>target/sierratestdatagenerator.sql</source>
			<outputDirectory>/</outputDirectory>
			<destName>sierratestdatagenerator.sql</destName>
		</file>
	</files>
</assembly>
