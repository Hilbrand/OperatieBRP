<!--
  - Additionele Binding informatie specifiek voor de tests. Hier worden abstracte types uit de normale model binding
  - in specifieke test mappings gebruikt zodat deze ook kunnen worden getest. Dit daar je niet direct abstracte
  - types kunt testen.
  -->
<binding>
    <namespace uri="http://www.bprbzk.nl/BRP/0001" default="all" prefix="brp"/>
    <namespace uri="http://www.kinggemeenten.nl/StUF/StUF0302" default="none" prefix="stuf"/>

    <include path="classpath:/binding-objecttypen-model.xml" precompiled="false"/>
    <include path="classpath:/binding-stuf.xml" precompiled="false"/>

    <mapping name="Objecttype_PersoonVoornaam"
             class="nl.bzk.brp.model.operationeel.kern.PersoonVoornaamModel">
        <structure map-as="Objecttype_PersoonVoornaam"/>
    </mapping>

    <mapping name="Objecttype_PersoonAdres"
             class="nl.bzk.brp.model.operationeel.kern.PersoonAdresModel">
        <structure map-as="Objecttype_PersoonAdres"/>
    </mapping>

    <mapping name="StuurgegevensDu02_VraagTEST" class="nl.bzk.brp.model.bericht.ber.BerichtStuurgegevensGroepBericht">
        <structure map-as="StuurgegevensDu02_Vraag" ns="http://www.kinggemeenten.nl/StUF/StUF0302"/>
    </mapping>

</binding>
