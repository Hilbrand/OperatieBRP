<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="nl.bzk.brp.testdatageneratie.domain.bronnen.AdresHuisnummertoevoeging" table="adres_huisnummertoevoeging">
        <id name="waarde" type="string">
            <column name="waarde" />
            <generator class="assigned" />
        </id>
        <property name="aantal" type="int">
            <column name="aantal" not-null="true" />
        </property>
        <property name="van" type="int">
            <column name="van" />
        </property>
        <property name="totEnMet" type="int">
            <column name="tot_en_met" />
        </property>
    </class>
</hibernate-mapping>
