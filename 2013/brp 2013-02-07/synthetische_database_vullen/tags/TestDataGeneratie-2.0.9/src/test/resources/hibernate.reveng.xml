<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

    <table-filter match-name=".*"/>

    <table name="pers">
        <primary-key>
            <generator class="sequence">
                <param name="sequence">kern.seq_pers</param>
            </generator>
            <key-column name="id"/>
        </primary-key>
    </table>

    <table name="persadres">
        <primary-key>
            <generator class="sequence">
                <param name="sequence">kern.seq_PersAdres</param>
            </generator>
            <key-column name="id"/>
        </primary-key>
    </table>

    <table name="persindicatie">
        <primary-key>
            <generator class="sequence">
                <param name="sequence">kern.seq_persindicatie</param>
            </generator>
            <key-column name="id"/>
        </primary-key>
    </table>
</hibernate-reverse-engineering>