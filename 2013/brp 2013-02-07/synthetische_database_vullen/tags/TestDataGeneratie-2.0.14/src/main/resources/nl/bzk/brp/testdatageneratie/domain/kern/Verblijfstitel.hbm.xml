<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.bzk.brp.testdatageneratie.domain.kern.Verblijfstitel" table="verblijfstitel" schema="kern">
        <id name="id" type="short">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="oms" type="string">
            <column name="oms" length="250" not-null="true" unique="true" />
        </property>
        <property name="code" type="short">
            <column name="code" not-null="true" />
        </property>
        <property name="dataanvgel" type="java.lang.Integer">
            <column name="dataanvgel" />
        </property>
        <property name="dateindegel" type="java.lang.Integer">
            <column name="dateindegel" />
        </property>
    </class>
</hibernate-mapping>
